<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:SWGPatcher" x:Class="SWGPatcher.MainWindow"
        Title="MainWindow" Height="343" Width="517" SizeToContent="WidthAndHeight" ResizeMode="NoResize">
    <Grid>
        <ProgressBar HorizontalAlignment="Left" Height="51" Margin="10,156,0,0" VerticalAlignment="Top" Width="497"/>
        <TextBox HorizontalAlignment="Left" Height="98" Margin="10,212,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="497"/>
        <Image Margin="10,10,368,171" Source="Image1.png" Stretch="Fill" Height="133" Width="133"/>
        <Button x:Name="launchButton" Content="Launch" HorizontalAlignment="Left" Margin="387,123,0,0" VerticalAlignment="Top" Width="116" Height="26" Click="launchButtonClicked" IsEnabled="False"/>
        <Button x:Name="setDirectoryButton" Content="Set SWG Directory" HorizontalAlignment="Left" Margin="387,94,0,0" VerticalAlignment="Top" Width="116" Height="24" Click="setDirectoryButtonClicked" RenderTransformOrigin="0.491,0.583"/>
        <TextBox x:Name="swgDirectoryDisplay" Height="24" Margin="148,94,127,0" TextWrapping="Wrap" VerticalAlignment="Top" IsEnabled="False" Text="{Binding Path=swgDirectory, Mode=TwoWay}"/>


    </Grid>
</Window>
